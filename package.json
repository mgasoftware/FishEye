{
  "scripts": {
    "lint": "eslint --max-warnings=0 ."
  },
  "plugins": [
    "no-unused-vars-rest"
  ],
  "rules": {
    "eqeqeq": "warn",
    "indent": ["warn", 2, { "SwitchCase": 1 }],
    "multiline-ternary": ["warn", "always-multiline"],
    "no-unused-vars": 0,
    "no-unused-vars-rest/no-unused-vars": [2, {"ignoreDestructuredVarsWithRest": true}]

  }
}